import java.io.*;
import java.net.*;
import java.util.*;

public class iperfer_client {

	final int port;
	final int time;
	final String host;	

	public iperfer_client (int port, int time, String host) throws IOException {
		this.port = port;
		this.host = host;
		this.time = time;
	}

	public void send_data (int time) {
		Byte[] data = null;


		data = new Byte[1000];
		Arrays.fill(data, (byte)0);
		Socket s = new Socket(this.host, this.port);
	}	
	
}
